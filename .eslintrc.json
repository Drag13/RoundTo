{
    "extends": [
        "google",
        "prettier"
    ],
    "overrides": [
        {
            "files": [
                ""
            ],
            "excludedFiles": [
                "*.d.ts"
            ]
        }
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "es5",
                "singleQuote": true,
                "printWidth": 100
            }
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "max-len": [
            1,
            120
        ],
        "object-curly-spacing": [
            "error",
            "always"
        ]
    },
    "plugins": [
        "prettier"
    ]
}